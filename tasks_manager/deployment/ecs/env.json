{
    "environment": [
      {
        "name": "PYTHONUNBUFFERED",
        "value": "1"
      },
      {
        "name": "DEBUG",
        "value": "true"
      },
      {
        "name": "ENV",
        "value": "$env"
      }
    ],
    "secrets": [
      {
        "name": "PREFECT_API_URL",
        "valueFrom": "arn:aws:ssm:$region:$account:parameter/fulcrum/PREFECT_API_URL"
      },
      {
        "name": "PREFECT_API_KEY",
        "valueFrom": "arn:aws:ssm:$region:$account:parameter/fulcrum/PREFECT_API_TOKEN"
      }
    ]
  }

