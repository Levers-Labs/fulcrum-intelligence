{
  "environment": [
    {
      "name": "PYTHONUNBUFFERED",
      "value": "1"
    },
    {
      "name": "DEBUG",
      "value": "true"
    },
    {
      "name": "ENV",
      "value": "$env"
    },
    {
      "name": "AWS_REGION",
      "value": "$region"
    },
    {
      "name": "PGSSLMODE",
      "value": "require"
    }
  ],
  "secrets": [
    {
      "name": "SERVER_HOST",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/SERVER_HOST"
    },
    {
      "name": "STORY_MANAGER_SERVER_HOST",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/STORY_MANAGER_SERVER_HOST"
    },
    {
      "name": "ANALYSIS_MANAGER_SERVER_HOST",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/ANALYSIS_MANAGER_SERVER_HOST"
    },
    {
      "name": "QUERY_MANAGER_SERVER_HOST",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/QUERY_MANAGER_SERVER_HOST"
    },
    {
      "name": "INSIGHTS_BACKEND_SERVER_HOST",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/INSIGHTS_BACKEND_SERVER_HOST"
    },
    {
      "name": "DATABASE_URL",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/fulcrum/DATABASE_URL"
    },
    {
      "name": "SECRET_KEY",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/fulcrum/SECRET_KEY"
    },
    {
      "name": "AUTH0_API_AUDIENCE",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/fulcrum/AUTH0_API_AUDIENCE"
    },
    {
      "name": "AUTH0_ISSUER",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/fulcrum/AUTH0_ISSUER"
    },
    {
      "name": "AUTH0_CLIENT_ID",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/AUTH0_CLIENT_ID"
    },
    {
      "name": "AUTH0_CLIENT_SECRET",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/AUTH0_CLIENT_SECRET"
    },
    {
      "name": "DAGSTER_PG_HOST",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/DAGSTER_PG_HOST"
    },
    {
      "name": "DAGSTER_PG_PORT",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/DAGSTER_PG_PORT"
    },
    {
      "name": "DAGSTER_PG_DB",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/DAGSTER_PG_DB"
    },
    {
      "name": "DAGSTER_PG_USER",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/DAGSTER_PG_USER"
    },
    {
      "name": "DAGSTER_PG_PASSWORD",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/DAGSTER_PG_PASSWORD"
    },
    {
      "name": "DAGSTER_S3_BUCKET",
      "valueFrom": "arn:aws:ssm:$region:$account:parameter/$app/DAGSTER_S3_BUCKET"
    }
  ]
}
